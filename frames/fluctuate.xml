      <example name="arg2 as manner" src="">
        <text>Nashua 's stock has fluctuated sharply on takeover speculation .</text>
        <propbank>
          <rel relloc="4">fluctuated</rel>
          <arg type="ARG1" start="0" end="2">Nashua 's stock</arg>
          <arg type="ARG2" start="5" end="5">sharply</arg>
