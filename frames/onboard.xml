<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE frameset PUBLIC "-//PB//PropBank Frame v3.4 Transitional//EN" "http://propbank.org/specification/dtds/v3.4/frameset.dtd">
<frameset>
  <predicate lemma="onboard">
    <roleset id="onboard.01" name="on a vessel/vehicle">
      <aliases>
        <alias pos="j">onboard</alias>
      </aliases>
      <roles>
        <role descr="entity who is onboard" f="PAG" n="1"/>
        <role descr="the 'vessel'" f="PPT" n="2"/>
      </roles>
      <usagenotes>
        <usage resource="PropBank" version="1.0" inuse="-"/>
        <usage resource="PropBank" version="2.1.5" inuse="-"/>
        <usage resource="PropBank" version="3.1" inuse="+"/>
        <usage resource="PropBank" version="3.4" inuse="+"/>
        <usage resource="AMR" version="2019" inuse="-"/>
        <usage resource="PropBank" version="Flickr 1.0" inuse="+"/>
        </usagenotes>
      <example name="onboard-j" src="">
        <text>I am onboard the Braemar , heading to the Dominican Republic .</text>
        <propbank>
          <rel relloc="2">onboard</rel>
          <arg type="ARG1" start="0" end="0">I</arg>
          <arg type="ARG2" start="3" end="4">the Braemar</arg>
          <arg type="ARGM-ADV" start="6" end="10">heading to the Dominican Republic</arg>
        </propbank>
      </example>
      <note>ONBOARD-J NOTES: Added by Julia based on SHARP (from onboard.01-j)</note>
    </roleset>
    <roleset id="onboard.02" name="in support of, going along with">
      <aliases>
        <alias pos="j">onboard</alias>
      </aliases>
      <roles>
        <role descr="entity who is supporting" f="PAG" n="1"/>
        <role descr="onboard with what, in support of" f="PPT" n="2"/>
      </roles>
      <usagenotes>
        <usage resource="PropBank" version="1.0" inuse="-"/>
        <usage resource="PropBank" version="2.1.5" inuse="-"/>
        <usage resource="PropBank" version="3.1" inuse="+"/>
        <usage resource="PropBank" version="3.4" inuse="+"/>
        <usage resource="AMR" version="2019" inuse="-"/>
        <usage resource="PropBank" version="Flickr 1.0" inuse="+"/>
        </usagenotes>
      <example name="onboard-j" src="">
        <text>I am onboard with Toddlers and Panda Bears .</text>
        <propbank>
          <rel relloc="2">onboard</rel>
          <arg type="ARG1" start="0" end="0">I</arg>
          <arg type="ARG2" start="3" end="7">with Toddlers and Panda Bears</arg>
        </propbank>
      </example>
      <example name="onboard-j" src="">
        <text>I am onboard the Kei Nishikori bandwagon .</text>
        <propbank>
          <rel relloc="2">onboard</rel>
          <arg type="ARG1" start="0" end="0">I</arg>
          <arg type="ARG2" start="3" end="6">the Kei Nishikori bandwagon</arg>
        </propbank>
      </example>
      <note>ONBOARD-J NOTES: Added by Julia for unification</note>
      <note>Notice how the argument for "toddlers and Panda Bears" would be completely different if this were the vessel sense.</note>
      <note>metaphorical example mirrors the .01 roleset, but means the .02 roleset</note>
    </roleset>
  </predicate>
</frameset>