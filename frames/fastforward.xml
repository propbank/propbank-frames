<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE frameset PUBLIC "-//PB//PropBank Frame v3.4 Transitional//EN" "http://propbank.org/specification/dtds/v3.4/frameset.dtd">
<frameset>
  <predicate lemma="fastforward">
    <roleset id="fastforward.01" name="move forward quickly, like on a media player">
      <aliases>
        <alias pos="v">fastforward</alias>
        <alias pos="v">fast-forward</alias>
      </aliases>
      <roles>
        <role descr="fast forwarder" f="PAG" n="0"/>
        <role descr="medium moved forward" f="PPT" n="1"/>
        <role descr="amount moved forward" f="EXT" n="2"/>
        <role descr="start point" f="DIR" n="3"/>
        <role descr="end point" f="GOL" n="4"/>
      </roles>
      <usagenotes>
        <usage resource="PropBank" version="1.0" inuse="-"/>
        <usage resource="PropBank" version="2.1.5" inuse="-"/>
        <usage resource="PropBank" version="3.1" inuse="+"/>
        <usage resource="PropBank" version="3.4" inuse="+"/>
        <usage resource="AMR" version="2019" inuse="+"/>
        <usage resource="PropBank" version="Flickr 1.0" inuse="+"/>
        </usagenotes>
      <lexlinks/>
      <example name="fast_forward-m: args 0 and 2" src="BOLT">
        <text>Fast forward about 7 years and OH and I get back in touch after finding out that we live near each other .</text>
        <propbank>
          <rel relloc="0 1">Fast forward</rel>
          <arg type="ARG2" start="2" end="4">about 7 years</arg>
        </propbank>
      </example>
      <example name="fast_forward-m: args 0, 1, 4" src="internet">
        <text>He fast forwarded the cassette right to the end .</text>
        <propbank>
          <rel relloc="1 2">fast forwarded</rel>
          <arg type="ARG0" start="0" end="0">He</arg>
          <arg type="ARG1" start="3" end="4">the cassette</arg>
          <arg type="ARG4" start="5" end="8">right to the end</arg>
        </propbank>
      </example>
      <example name="fastforward-v: args 0, 1, 2, no concatenation needed" src="internet">
        <text>He fastforwarded the song a bit then handed an earbud to her .</text>
        <propbank>
          <rel relloc="1">fastforwarded</rel>
          <arg type="ARG0" start="0" end="0">He</arg>
          <arg type="ARG1" start="2" end="3">the song</arg>
          <arg type="ARG2" start="4" end="5">a bit</arg>
        </propbank>
      </example>
      <note>FASTFORWARD-V NOTES: Added by Julia based on BOLT. No VNcls. (from fastforward.01-v)</note>
    </roleset>
  </predicate>
</frameset>