<?xml version="1.0" encoding="utf-8" standalone="no"?>
<!DOCTYPE frameset PUBLIC "-//PB//PropBank Frame v3.4 Transitional//EN" "http://propbank.org/specification/dtds/v3.4/frameset.dtd">
<frameset>
  <predicate lemma="hotspot">
    <roleset id="hotspot.01" name="turn into a wifi hotspot">
      <aliases>
        <alias pos="v">hotspot</alias>
      </aliases>
      <roles>
        <role descr="agent" f="PAG" n="0"/>
        <role descr="location becoming a hotspot" f="PPT" n="1"/>
      </roles>
      <usagenotes>
        <usage resource="PropBank" version="1.0" inuse="-"/>
        <usage resource="PropBank" version="2.1.5" inuse="-"/>
        <usage resource="PropBank" version="3.1" inuse="+"/>
        <usage resource="PropBank" version="3.4" inuse="+"/>
        <usage resource="AMR" version="2019" inuse="-"/>
        <usage resource="PropBank" version="Flickr 1.0" inuse="+"/>
        </usagenotes>
      <example name="hotspot-v" src="">
        <text>I rooted and hotspotted the phone relatively soon after buying it , seemingly without any problems</text>
        <propbank>
          <rel relloc="3">hotspotted</rel>
          <arg type="ARG0" start="0" end="0">I</arg>
          <arg type="ARG1" start="4" end="5">the phone</arg>
          <arg type="ARGM-TMP" start="6" end="10">relatively soon after buying it</arg>
          <arg type="ARGM-MNR" start="12" end="15">seemingly without any problems</arg>
        </propbank>
      </example>
      <note>HOTSPOT-V NOTES: Added by Julia</note>
    </roleset>
    <roleset id="hotspot.02" name="create a web link in a certain location (the location being the hotspot)">
      <aliases>
        <alias pos="v">hotspot</alias>
      </aliases>
      <roles>
        <role descr="web designer" f="PAG" n="0"/>
        <role descr="location/word turned into a link" f="PPT" n="1"/>
        <role descr="what the link directs to" f="PPT" n="2"/>
      </roles>
      <usagenotes>
        <usage resource="PropBank" version="1.0" inuse="-"/>
        <usage resource="PropBank" version="2.1.5" inuse="-"/>
        <usage resource="PropBank" version="3.1" inuse="+"/>
        <usage resource="PropBank" version="3.4" inuse="+"/>
        <usage resource="AMR" version="2019" inuse="-"/>
        <usage resource="PropBank" version="Flickr 1.0" inuse="+"/>
        </usagenotes>
      <example name="hotspot-v" src="">
        <text>I hotspotted the points , hyperlinked to the relevant files , chose the lightbox option and added a caption .</text>
        <propbank>
          <rel relloc="1">hotspotted</rel>
          <arg type="ARG0" start="0" end="0">I</arg>
          <arg type="ARG1" start="2" end="3">the points</arg>
        </propbank>
      </example>
      <note>HOTSPOT-V NOTES: Added by Julia</note>
    </roleset>
  </predicate>
</frameset>