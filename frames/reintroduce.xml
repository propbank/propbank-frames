      <example name="without arg2" src="">
        <text>Mr. Stoltzman reintroduced his colleagues .</text>
        <propbank>
          <rel relloc="2">reintroduced</rel>
          <arg type="ARG0" start="0" end="1">Mr. Stoltzman</arg>
