      <example name="copay-v" src="">
        <text>For example , some patients will be required to copay $ 15 for a regular check up</text>
        <propbank>
          <rel relloc="9">copay</rel>
          <arg type="ARG0" start="3" end="4">some patients</arg>
          <arg type="ARG1" start="10" end="11">$ 15</arg>
          <arg type="ARG3" start="12" end="16">for a regular check up</arg>
        </propbank>
      </example>
