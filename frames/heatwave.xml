<frameset>
  <predicate lemma="heatwave">
    <roleset id="heatwave.01" name="natural disaster: stretch of very high temperatures">
      <aliases>
        <alias pos="n">heatwave</alias>
      </aliases>
      <roles>
        <role descr="temperature" f="EXT" n="1"/>
        <role descr="humidity" f="EXT" n="2"/>
        <role descr="death/injury toll" f="VSP" n="3"/>
