          <rel relloc="1">simulated</rel>
          <arg type="ARG0" start="0" end="0">John</arg>
          <arg type="ARG1" start="2" end="4">a VAX ministation</arg>
          <arg type="ARG2" start="5" end="19">with a cardboard box , three or four egg cartons , and a live beaver</arg>
        </propbank>
