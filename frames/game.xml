        <usage resource="PropBank" version="3.5" inuse="+"/>
        <usage resource="AMR" version="iSAT 1.0" inuse="+"/>
      </usagenotes>
      <lexlinks/>
      <example name="both args" src="SMS">
        <text>Future Facilities would be game for the Webinar at $ 8995 . Ok ? I am sending a proposal tonite do please let me know if $ is OK . Thanks Jan</text>
        <propbank>
          <rel relloc="4">game</rel>
          <arg type="ARG0" start="0" end="1">Future Facilities</arg>
          <arg type="ARGM-MOD" start="2" end="2">would</arg>
          <arg type="ARG1" start="5" end="7">for the Webinar</arg>
          <arg type="ARGM-ADV" start="8" end="10">at $ 8995</arg>
        </propbank>
      </example>
      <note>GAME-J NOTES: Added by Julia based on SMS. (from game.01-j)</note>
    </roleset>
  </predicate>
