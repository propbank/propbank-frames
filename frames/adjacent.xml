          <rel relloc="2">adjacent</rel>
          <arg type="ARG1" start="1" end="1">them</arg>
          <arg type="ARG2" start="3" end="7">to those you just removed</arg>
        </propbank>
        <amr version="Spatial 1.0">
          (p / place-01 :mode imperative
            :ARG0 (y / you)
            :ARG1 (t / they)
            :ARG3 (s / space :pl +
              :ARG1-of (a / adjacent-01
                :ARG2 (t2 / thing :pl +
                  :mod (t3 / that)
                  :ARG1-of (r / remove-01
                    :ARG0 y
                    :time (j / just))))))
