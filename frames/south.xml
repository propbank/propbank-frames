            :ARG1 (t / thing
              :ARG1-of (b / base-03
                :ARG3 (c3 / cartesian-framework-91
                  :ARG1 b5)))
            :ARG2 (b2 / block
              :color (b3 / blue)
              :ARG1-of (r / right-10
                :ARG3 (c2 / cartesian-framework-91
                  :ARG1 (b6 / builder))
                :ARG2-of (h / have-degree-91
                  :ARG1 b2
                  :ARG3 (m / most))))
            :ARG3 (c / cartesian-framework-91
              :ARG1 (b5 / build-space)
