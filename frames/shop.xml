        <lexlink class="search-35.2" confidence="1.0" resource="VerbNet" src="manualchecks" version="verbnet3.4"/>
      </lexlinks>
      <example name="intransitive" src="">
        <text>The number of people who said they were more likely to shop at Sears ...</text>
        <propbank>
          <rel relloc="11">shop</rel>
          <arg type="ARG0" start="6" end="6">they</arg>
          <arg type="ARG2" start="12" end="13">at Sears</arg>
        </propbank>
